#include <stdio.h>
int main() {
    int a[] = {2, 5, 8, 11, 14};
    int size = sizeof(a) / sizeof(a[0]);
    for (int i = 0; i < size; i++) {
        if (a[i] % 2 == 0) {
            printf("%d is even.\n", a[i]);
        } else {
            printf("%d is odd.\n", a[i]);
        }
    }
    return 0;
}
